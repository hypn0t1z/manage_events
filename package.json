    "debug": "^4.1.1",
    "express": "^4.17.1",
    "joi": "^17.4.1",
    "joi-objectid": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.6",
    "morgan": "^1.10.0",
    "winston": "^3.3.3"
